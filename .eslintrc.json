{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "eol-last": ["error", "never"],
      "import/prefer-default-export": "warn",
      "import/extensions": "off",
      "camelcase": "off",
      "comma-dangle": "off",
      "no-global-assign": "warn",
      "no-console": "off"
    }
}
